&GLOBAL
  PROJECT optbas
  PROGRAM_NAME OPTIMIZE_BASIS
  PRINT_LEVEL HIGH
&END GLOBAL

&OPTIMIZE_BASIS
   BASIS_TEMPLATE_FILE BASIS_SET_TEMPLATE
   BASIS_WORK_FILE WORK_BASIS_STRUCTURE
   BASIS_OUTPUT_FILE La_FIT_temp
   WRITE_FREQUENCY 10
   &OPTIMIZATION
      MAX_FUN 1000000
   &END
   &TRAINING_FILES
      DIRECTORY ../lacl3
      INPUT_FILE_NAME lacl3.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../lacl2
      INPUT_FILE_NAME lacl2.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../lacl
      INPUT_FILE_NAME lacl.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../laf3
      INPUT_FILE_NAME laf3.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../laf2
      INPUT_FILE_NAME laf2.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../laf
      INPUT_FILE_NAME laf.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../lao
      INPUT_FILE_NAME lao.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../lan
      INPUT_FILE_NAME lan.inp
   &END
   &TRAINING_FILES
      DIRECTORY ../lah
      INPUT_FILE_NAME lah.inp
   &END
   &FIT_KIND La
      BASIS_SET DZVP
      INITIAL_DEGREES_OF_FREEDOM COEFFICIENTS
  &END FIT_KIND
&END OPTIMIZE_BASIS
