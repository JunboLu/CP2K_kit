&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME ./BASIS_SET
    POTENTIAL_FILE_NAME ./POTENTIAL
    WFN_RESTART_FILE_NAME ./laf-RESTART.wfn
    UKS  T
    MULTIPLICITY  3
    &MGRID
      CUTOFF 800
      NGRIDS 3
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-12
    &END QS
    &SCF
      EPS_SCF 1.0E-7
      MAX_SCF 2000
#      SCF_GUESS ATOMIC
      SCF_GUESS RESTART
      &OT
       MINIMIZER CG
       LINESEARCH 3PNT
       PRECONDITIONER FULL_ALL
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
    &PRINT
    &END
  &END DFT
  &SUBSYS
    &CELL
      ABC 15.0 15.0 15.0
      PERIODIC NONE
    &END CELL
    &COORD
 La         7.5198261811        7.5475943551        8.5692916821
  F         7.4999869934        7.4414493989        6.5040648371
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES
      &END
    &END
    &KIND La
      BASIS_SET CBS
      POTENTIAL GTH-PBE-q11
    &END KIND
    &KIND F
      BASIS_SET TZV2P-MOLOPT-GTH
      POTENTIAL GTH-PBE-q7
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT laf
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
