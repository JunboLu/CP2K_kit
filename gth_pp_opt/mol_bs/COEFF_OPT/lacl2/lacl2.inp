&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME ./BASIS_SET
    POTENTIAL_FILE_NAME ./POTENTIAL
    WFN_RESTART_FILE_NAME ./lacl2-RESTART.wfn
    UKS  T
    MULTIPLICITY  2
    &MGRID
      CUTOFF 800
      NGRIDS 3
    &END MGRID
     &QS
      EPS_DEFAULT 1.0E-12
    &END QS
    &SCF
      EPS_SCF 1.0E-7
      MAX_SCF 2000
#      SCF_GUESS ATOMIC
      SCF_GUESS RESTART
      &OT
       MINIMIZER CG
       LINESEARCH 3PNT
       PRECONDITIONER FULL_ALL
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 15.0 15.0 15.0
      PERIODIC NONE
    &END CELL
    &COORD
 La         7.1575158350        7.5116568525        8.4361020761
 Cl         8.6328463025        9.3354846272        7.4173415430
 Cl         6.5148870205        5.6591356977        6.7938879241
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES
      &END
    &END
    &KIND La
      BASIS_SET CBS
      POTENTIAL GTH-PBE-q11
    &END KIND
    &KIND Cl
      BASIS_SET TZV2P-MOLOPT-GTH
      POTENTIAL GTH-PBE-q7
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT lacl2
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

